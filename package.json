{
  "name": "9gag",
  "version": "1.0.1",
  "description": "Download 9GAG posts and comments",
  "main": "src/Main.js",
  "types": "src/Main.d.ts",
  "scripts": {
    "start": "node src/Main",
    "test": "mocha --timeout 5000 --recursive tests",
    "test:unit": "mocha tests/unit",
    "test:integration": "mocha --timeout 5000 tests/integration",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --timeout 5000 --recursive tests",
    "build": "tsc -p .",
    "watch": "tsc -w -p ."
