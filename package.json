    "fs-extra": "^2.0.0",
    "handlebars": "^4.0.6",
    "request": "^2.79.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/cheerio": "^0.17.31",
    "@types/fs-extra": "0.0.37",
    "@types/handlebars": "^4.0.31",
    "@types/mocha": "^2.2.39",
    "@types/node": "^6.0.54",
    "@types/request": "0.0.39",
    "@types/sinon": "^1.16.34",
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
